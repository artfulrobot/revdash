<?xml version="1.0"?>
<extension key="revdash" type="module">
  <file>revdash</file>
  <name>Revenue Dashboard</name>
  <description>CiviCRM extension to provide some up-to-date charts and metrics on revenue.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/revdash</url>
    <url desc="Documentation">https://github.com/artfulrobot/revdash</url>
    <url desc="Support">https://github.com/artfulrobot/revdash</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-02-27</releaseDate>
  <version>1.0</version>
  <develStage>beta`</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is developed for my clients. You may need to tweak it to your own implementation.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Revdash</namespace>
  </civix>
</extension>
